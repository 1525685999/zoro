{
  "name": "roronoa-zoro",
  "version": "1.2.4",
  "description": "a redux framework, complete async await",
  "main": "index.js",
  "keywords": [
    "wx",
    "wx-redux",
    "weapp",
    "redux",
    "roronoa",
    "zoro",
    "effect",
    "promise",
    "framework",
    "frontend"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:es && npm run build:cjs && npm run clean:free",
    "build:es1": "rollup --config rollup.config.es.js",
    "build:es": "babel src --out-dir ./ --copy-files",
    "build:cjs": "rollup --config rollup.config.cjs.js",
    "clean": "rm -rf ./dist ./lib ./example.js ./index.js ./plugin.js",
    "clean:free": "rm -rf ./regenerator.js ./weapp-redux.js",
    "dev": "babel src --out-dir ./ --copy-files -w",
    "example": "npm run build && node ./example.js",
    "release": "npm run lint && npm run build && npm publish",
    "lint": "eslint src",
    "test": "jest"
  },
  "author": {
    "name": "Faure Wu",
    "email": "502556093@qq.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/FaureWu/zoro.git",
  "dependencies": {
    "redux": "^4.0.0",
    "regenerator-runtime": "^0.12.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.51",
    "@babel/core": "^7.0.0-beta.52",
    "@babel/plugin-proposal-async-generator-functions": "^7.0.0-beta.55",
    "@babel/preset-env": "^7.0.0-beta.51",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^23.4.0",
    "cross-env": "^5.2.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "jest": "^23.4.0",
    "node-dev": "^3.1.3",
    "prettier": "^1.13.5",
    "rollup": "^0.63.4",
    "rollup-plugin-babel": "^4.0.0-beta.7",
    "rollup-plugin-commonjs": "^9.1.4",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0"
  }
}
